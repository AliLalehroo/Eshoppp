@if (TempData["SuccessMessage"] != null)
{
    <script>
        $(document).ready(function () {
            swal.fire({
                title: "پیغام موفقیت",
                text: '@TempData["SuccessMessage"]',
                icon: "success",
                width: '28em',
                confirmButtonText: "باشه"
            });
        });
    </script>
}

